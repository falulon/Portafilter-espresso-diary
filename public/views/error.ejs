<% layout('layouts/boilerplate')%>
    <div class="row justify-content-center">

        <div class="col-md-6">
            <h2>Contact Support</h2>

            <img src="https://images.unsplash.com/photo-1519174528871-df734940e8c7?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=634&q=80"
                alt="error" class="img-fluid">

            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading"></h4>
                <p>
                    <%=err.message%>
                </p>
                <img src="https://images.unsplash.com/photo-1578651714116-2a1645b70c0f?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1267&q=80"
                    alt="error" class="img-fluid">
                <% if (process.env.NODE_ENV !=="production" ) { %>
                    <p>
                        <%= err.stack %>
                    </p>
                    <% } %>
            </div>
        </div>
    </div>